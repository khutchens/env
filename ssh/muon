Host mfc
    User root
    StrictHostKeyChecking no
    UserKnownHostsFile /dev/null

Host muon-*
    User kyle
    IdentityFile ~/.ssh/id-muon_lab_sso
    AddKeysToAgent yes
    ForwardAgent yes

    CanonicalizeHostname yes
    CanonicalDomains labs.muonspace.com

    ControlPath ~/.ssh/control-master-sockets/%r@%h:%p
    ControlMaster auto
    ControlPersist 10m

    # Alternate configs for logging in as muon user:
    #User muon
    #IdentityFile ~/.ssh/id-muon_proxy

    # Alternate configs for Ansible runs:
    #User muon
    #IdentityFile ~/.ssh/id-muon_lab_shared

Host ms2-egse
    User kyle
    IdentityFile ~/.ssh/id-muon_lab_sso
    Hostname muon-egse-cart-1
    ProxyJump muon-lab1

    ControlPath ~/.ssh/control-master-sockets/%r@%h:%p
    ControlMaster auto
    ControlPersist 10m
